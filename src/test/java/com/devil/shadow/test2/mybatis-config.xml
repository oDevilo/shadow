    <!--<plugin interceptor="com.ys.mybatis.shadow.plugin.ShadowPlugin">-->
    <!--<property name="shadowConfig" value="com/ys/mybatis/shadow/shadow_config.xml" />-->
    <!--</plugin>-->
    <!--</plugins>-->
</configuration>